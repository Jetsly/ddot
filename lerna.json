{
  "useWorkspaces": true,
  "version": "independent",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "ignoreChanges": [
        "docs",
        "*.html",
        "*.md"
      ]
    }
  },
  "changelog": {
    "labels": {
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":book: Documentation"
    },
    "repo": "Jetsly/ddot",
    "cacheDir": ".changelog"
  },
  "npmClient": "yarn"
}
